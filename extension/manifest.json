{
	"manifest_version": 2,
	"name": "My Extension",
	"version": "1.0.0",
	// "default_locale": "en",
	"description": "WABLAM",
	"permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "https://*/*",
    "http://*/*",
    "https://www.instagram.com/*"
  ],
	"browser_action": {
		"default_icon": "icon32.png",
		"default_title": "RecallHunter"
	},
	"content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	}
	// "content_scripts": [
	// 	{
	// 		"js": ["./src/index.js"]
	// 	}
	// ]
}